        android:textColor="#353A36"
        android:textColorHint="#353A36"
        app:layout_constraintBottom_toBottomOf="@+id/ivPopup"
        app:layout_constraintEnd_toEndOf="@+id/ivPopup"
        app:layout_constraintHorizontal_bias="0.074"
        app:layout_constraintStart_toStartOf="@+id/ivPopup"
        app:layout_constraintTop_toBottomOf="@+id/ivTab"
        app:layout_constraintVertical_bias="0.166" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/work_sans_medium"
        android:text="PhilHealth:"
        android:textColor="#353A36"
        android:textColorHint="#353A36"
        app:layout_constraintStart_toStartOf="@+id/ivPopup"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/work_sans_medium"
        android:text="Pag-IBIG:"
        android:textColor="#353A36"
        android:textColorHint="#353A36"
        app:layout_constraintStart_toStartOf="@+id/ivPopup"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="36dp"
        android:fontFamily="@font/work_sans_medium"
        android:text="Total:"
        android:textColor="#353A36"
        android:textColorHint="#353A36"
        app:layout_constraintStart_toStartOf="@+id/ivPopup"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/work_sans_medium"
        android:text="Contributions"
        android:textColor="#00704A"
        app:layout_constraintBottom_toBottomOf="@+id/ivPopup"
        app:layout_constraintEnd_toEndOf="@+id/ivPopup"
        app:layout_constraintStart_toStartOf="@+id/ivPopup"
        app:layout_constraintTop_toBottomOf="@+id/ivTab"
