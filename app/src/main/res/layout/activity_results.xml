<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:name="Results"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:backgroundTint="#757E61"
    android:theme="@style/Theme.AppCompat.Translucent"
    android:visibility="visible"
    tools:context=".Results">

    <ImageView
        android:id="@+id/ivPopup"
        android:layout_width="300dp"
        android:layout_height="650dp"
        android:background="#EAEFD0"
        android:fadingEdge="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.481" />

    <ImageView
        android:id="@+id/ivTab"
        android:layout_width="300dp"
        android:layout_height="75dp"
        android:background="#00704A"
        android:fadingEdge="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ivPopup"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/editorial"
        android:text="Results"
        android:textColor="#EAEFD0"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="@+id/ivTab"
        app:layout_constraintEnd_toEndOf="@+id/ivTab"
        app:layout_constraintHorizontal_bias="0.075"
        app:layout_constraintStart_toStartOf="@+id/ivTab"
        app:layout_constraintTop_toTopOf="@+id/ivPopup"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/work_sans_medium"
        android:text="Monthly Income:"
        android:textColor="#353A36"
        android:textColorHint="#353A36"
        app:layout_constraintBottom_toBottomOf="@+id/ivPopup"
        app:layout_constraintEnd_toEndOf="@+id/ivPopup"
        app:layout_constraintHorizontal_bias="0.092"
        app:layout_constraintStart_toStartOf="@+id/ivPopup"
        app:layout_constraintTop_toBottomOf="@+id/ivTab"
        app:layout_constraintVertical_bias="0.028" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/work_sans_medium"
