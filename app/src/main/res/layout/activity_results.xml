        android:rotationY="0"
        android:scaleX="1"
        app:layout_constraintStart_toEndOf="@+id/btnBack"
        app:layout_constraintTop_toBottomOf="@+id/afterTax"
        app:srcCompat="@drawable/nwjsbunny" />


