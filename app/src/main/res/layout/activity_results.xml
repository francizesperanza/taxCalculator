        android:textColorHint="#353A36"
        app:layout_constraintEnd_toEndOf="@+id/ivPopup"
        app:layout_constraintHorizontal_bias="0.169"
        app:layout_constraintStart_toStartOf="@+id/ivPopup"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <Button
        android:id="@+id/btnBack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#00704A"
        android:fontFamily="@font/work_sans_medium"
        android:text="Back"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="@+id/ivPopup"
        app:layout_constraintEnd_toEndOf="@+id/ivPopup"
        app:layout_constraintStart_toStartOf="@+id/ivPopup"
        app:layout_constraintTop_toBottomOf="@+id/ivTab"
        app:layout_constraintVertical_bias="0.984" />

    <TextView
