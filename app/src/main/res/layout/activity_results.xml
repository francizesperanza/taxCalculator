<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:name="Results"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:backgroundTint="#E9EECF"
    android:theme="@style/Theme.TaxCalculator"
    android:visibility="visible"
    tools:context=".Results">

    <ImageView
        android:id="@+id/ivPopup"
        android:layout_width="300dp"
        android:layout_height="650dp"
        android:background="@drawable/textfield"
        android:fadingEdge="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.481" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#0000704A"
        android:fontFamily="@font/editorial"
        android:text="---------------------------"
        android:textColor="#00704A"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/ivPopup"
        app:layout_constraintEnd_toEndOf="@+id/ivPopup"
        app:layout_constraintStart_toStartOf="@+id/ivPopup"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        app:layout_constraintVertical_bias="0.389" />

    <ImageView
        android:id="@+id/ivTab"
        android:layout_width="300dp"
        android:layout_height="75dp"
        android:background="#00704A"
        android:fadingEdge="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ivPopup"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/editorial"
        android:text="Results"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="@+id/ivTab"
        app:layout_constraintEnd_toEndOf="@+id/ivTab"
        app:layout_constraintHorizontal_bias="0.075"
        app:layout_constraintStart_toStartOf="@+id/ivTab"
        app:layout_constraintTop_toTopOf="@+id/ivPopup"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/editorial"
        android:text="Monthly Income:"
        android:textColor="#353A36"
        android:textColorHint="#353A36"
        app:layout_constraintBottom_toBottomOf="@+id/ivPopup"
        app:layout_constraintEnd_toEndOf="@+id/ivPopup"
        app:layout_constraintHorizontal_bias="0.092"
        app:layout_constraintStart_toStartOf="@+id/ivPopup"
        app:layout_constraintTop_toBottomOf="@+id/ivTab"
        app:layout_constraintVertical_bias="0.028" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/editorial"
        android:text="SSS:"
        android:textColor="#353A36"
        android:textColorHint="#353A36"
        app:layout_constraintBottom_toBottomOf="@+id/ivPopup"
        app:layout_constraintEnd_toEndOf="@+id/ivPopup"
        app:layout_constraintHorizontal_bias="0.072"
        app:layout_constraintStart_toStartOf="@+id/ivPopup"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.043" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/editorial"
        android:text="PhilHealth:"
        android:textColor="#353A36"
        android:textColorHint="#353A36"
        app:layout_constraintStart_toStartOf="@+id/ivPopup"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/editorial"
        android:text="Pag-IBIG:"
        android:textColor="#353A36"
        android:textColorHint="#353A36"
        app:layout_constraintStart_toStartOf="@+id/ivPopup"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/editorial"
        android:text="Total:"
        android:textColor="#353A36"
        android:textColorHint="#353A36"
        app:layout_constraintStart_toStartOf="@+id/ivPopup"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#0000704A"
        android:fontFamily="@font/editorial"
        android:text="------- Contributions -------"
        android:textColor="#00704A"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/ivPopup"
        app:layout_constraintEnd_toEndOf="@+id/ivPopup"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="@+id/ivPopup"
        app:layout_constraintTop_toBottomOf="@+id/ivTab"
        app:layout_constraintVertical_bias="0.087" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#0000704A"
        android:fontFamily="@font/editorial"
        android:text="----- Tax Computations -----"
        android:textColor="#00704A"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/ivPopup"
        app:layout_constraintEnd_toEndOf="@+id/ivPopup"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="@+id/ivPopup"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.302" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/editorial"
        android:text="Taxable Income:"
        android:textColor="#353A36"
        android:textColorHint="#353A36"
        app:layout_constraintBottom_toBottomOf="@+id/ivPopup"
        app:layout_constraintEnd_toEndOf="@+id/ivPopup"
        app:layout_constraintHorizontal_bias="0.096"
        app:layout_constraintStart_toStartOf="@+id/ivPopup"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        app:layout_constraintVertical_bias="0.061" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/editorial"
        android:text="Income Tax:"
        android:textColor="#353A36"
        android:textColorHint="#353A36"
        app:layout_constraintStart_toStartOf="@+id/ivPopup"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/editorial"
        android:text="Net Pay After Tax:"
        android:textColor="#353A36"
        android:textColorHint="#353A36"
        app:layout_constraintStart_toStartOf="@+id/ivPopup"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/editorial"
        android:text="Total Deductions"
        android:textColor="#353A36"
        android:textColorHint="#353A36"
        app:layout_constraintBottom_toBottomOf="@+id/ivPopup"
        app:layout_constraintEnd_toEndOf="@+id/ivPopup"
        app:layout_constraintHorizontal_bias="0.099"
        app:layout_constraintStart_toStartOf="@+id/ivPopup"
        app:layout_constraintTop_toBottomOf="@+id/textView16"
        app:layout_constraintVertical_bias="0.132" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:fontFamily="@font/editorial"
        android:text="Net Pay After Deductions"
        android:textColor="#353A36"
        android:textColorHint="#353A36"
        app:layout_constraintEnd_toEndOf="@+id/ivPopup"
        app:layout_constraintHorizontal_bias="0.121"
        app:layout_constraintStart_toStartOf="@+id/ivPopup"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <Button
        android:id="@+id/btnBack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#FEFEFE"
        android:fontFamily="@font/editorial"
        android:text="Back"
        android:textAllCaps="false"
        android:textColor="#0B7B55"
        app:layout_constraintBottom_toBottomOf="@+id/ivPopup"
        app:layout_constraintEnd_toEndOf="@+id/ivPopup"
        app:layout_constraintStart_toStartOf="@+id/ivPopup"
        app:layout_constraintTop_toBottomOf="@+id/ivTab"
        app:layout_constraintVertical_bias="0.984"
        app:strokeColor="#0B7B55"
        app:strokeWidth="3px" />

    <TextView
        android:id="@+id/SSS"
        android:layout_width="107dp"
        android:layout_height="19dp"
        android:layout_marginStart="110dp"
        android:layout_marginTop="20dp"
        android:fontFamily="serif-monospace"
        android:text="TextView"
        android:textAlignment="textEnd"
        android:textColor="#353A36"
        android:textColorHint="#353A36"
        app:layout_constraintEnd_toEndOf="@+id/ivPopup"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/monthlyInc"
        android:layout_width="157dp"
        android:layout_height="19dp"
        android:layout_marginTop="17dp"
        android:fontFamily="serif-monospace"
        android:text="TextView"
        android:textAlignment="textEnd"
        android:textColor="#353A36"
        app:layout_constraintEnd_toEndOf="@+id/ivPopup"
        app:layout_constraintHorizontal_bias="0.485"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/ivTab" />

    <TextView
        android:id="@+id/PhilHealth"
        android:layout_width="107dp"
        android:layout_height="19dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="10dp"
        android:fontFamily="serif-monospace"
        android:text="TextView"
        android:textAlignment="textEnd"
        android:textColor="#353A36"
        app:layout_constraintEnd_toEndOf="@+id/ivPopup"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/SSS" />

    <TextView
        android:id="@+id/PagIBIG"
        android:layout_width="107dp"
        android:layout_height="19dp"
        android:layout_marginTop="10dp"
        android:fontFamily="serif-monospace"
        android:text="TextView"
        android:textAlignment="textEnd"
        android:textColor="#353A36"
        app:layout_constraintEnd_toEndOf="@+id/ivPopup"
        app:layout_constraintHorizontal_bias="0.85"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/PhilHealth" />

    <TextView
        android:id="@+id/totalCont"
        android:layout_width="107dp"
        android:layout_height="19dp"
        android:layout_marginTop="10dp"
        android:fontFamily="serif-monospace"
        android:text="TextView"
        android:textAlignment="textEnd"
        android:textColor="#353A36"
        app:layout_constraintEnd_toEndOf="@+id/ivPopup"
        app:layout_constraintHorizontal_bias="0.874"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/PagIBIG" />

    <TextView
        android:id="@+id/Taxable"
        android:layout_width="157dp"
        android:layout_height="19dp"
        android:layout_marginTop="19dp"
        android:fontFamily="serif-monospace"
        android:text="TextView"
        android:textAlignment="textEnd"
        android:textColor="#353A36"
        app:layout_constraintEnd_toEndOf="@+id/ivPopup"
        app:layout_constraintHorizontal_bias="0.525"
        app:layout_constraintStart_toEndOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <TextView
        android:id="@+id/IncomeTax"
        android:layout_width="157dp"
        android:layout_height="19dp"
        android:layout_marginTop="8dp"
        android:fontFamily="serif-monospace"
        android:text="TextView"
        android:textAlignment="textEnd"
        android:textColor="#353A36"
        app:layout_constraintEnd_toEndOf="@+id/ivPopup"
        app:layout_constraintHorizontal_bias="0.672"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/Taxable" />

    <TextView
        android:id="@+id/afterTax"
        android:layout_width="157dp"
        android:layout_height="19dp"
        android:layout_marginTop="10dp"
        android:fontFamily="serif-monospace"
        android:text="TextView"
        android:textAlignment="textEnd"
        android:textColor="#393E39"
        app:layout_constraintEnd_toEndOf="@+id/ivPopup"
        app:layout_constraintHorizontal_bias="0.208"
        app:layout_constraintStart_toEndOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/IncomeTax" />

    <TextView
        android:id="@+id/totalDeduct"
        android:layout_width="157dp"
        android:layout_height="19dp"
        android:layout_marginTop="5dp"
        android:fontFamily="serif-monospace"
        android:text="TextView"
        android:textAlignment="textEnd"
        android:textColor="#D81111"
        app:layout_constraintEnd_toEndOf="@+id/ivPopup"
        app:layout_constraintHorizontal_bias="0.867"
        app:layout_constraintStart_toStartOf="@+id/ivPopup"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <TextView
        android:id="@+id/afterDeduct1"
        android:layout_width="157dp"
        android:layout_height="19dp"
        android:layout_marginTop="56dp"
        android:fontFamily="serif-monospace"
        android:text="TextView"
        android:textAlignment="textEnd"
        android:textColor="#00704A"
        app:layout_constraintEnd_toEndOf="@+id/ivPopup"
        app:layout_constraintHorizontal_bias="0.867"
        app:layout_constraintStart_toStartOf="@+id/ivPopup"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="177dp"
        android:layout_height="412dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="52dp"
        android:rotation="15"
        android:rotationY="0"
        android:scaleX="1"
        app:layout_constraintStart_toEndOf="@+id/btnBack"
        app:layout_constraintTop_toBottomOf="@+id/afterTax"
        app:srcCompat="@drawable/nwjsbunny" />


</androidx.constraintlayout.widget.ConstraintLayout>